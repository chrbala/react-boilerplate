{
	"name": "boilerplate",
	"version": "0.0.1",
	"description": "Boilerplate for react apps",
	"main": "index.js",
	"scripts": {
		"preinstall": "rm -rf ./app",
		"postinstall": "gulp",
		"gulp": "gulp",
		"start": "node start",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"boilerplate",
		"react"
	],
