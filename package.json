		"parse-filepath": "0.6.3",
		"react": "0.14.3",
		"react-dom": "0.14.3",
		"react-router": "1.0.3",
		"redux": "3.0.5",
		"redux-simple-router": "1.0.2",
		"vinyl-source-stream": "1.1.0"
	},
	"engines": {
		"node": "4.2.1"
	},
	"aliasify": {
		"aliases": {
			"components": "./src/components",
			"shared": "./src/shared",
			"packages": "./src/packages",
			"store": "./src/store",
			"root": "/"
		}
	},
	"babel": {
