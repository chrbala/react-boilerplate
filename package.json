	"license": "ISC",
	"repository": "https://github.com/chrbala/react-boilerplate",
	"dependencies": {
		"aliasify": "1.8.0",
		"babel-core": "6.3.26",
		"babel-eslint": "4.1.6",
		"babel-polyfill": "6.3.14",
		"babel-preset-es2015": "6.3.13",
		"babel-preset-react": "6.3.13",
		"babel-preset-stage-0": "6.3.13",
		"babelify": "7.2.0",
		"bluebird": "3.1.1",
		"body-parser": "1.14.2",
		"concurrently": "1.0.0",
		"eslint": "1.10.3",
		"express": "4.13.3",
		"gulp": "3.9.0",
		"gulp-browserify": "0.5.1",
		"gulp-ignore": "2.0.1",
		"gulp-livereload": "3.8.1",
